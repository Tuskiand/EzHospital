.page-backgroud{width:100%;height:100%;background:#f2f2f2}.visited-color{color:#7ec0ee!important}.cut-long-text{text-overflow:ellipsis;white-space:nowrap;overflow:hidden}.tips-box{width:100%;height:100rpx;border:1px solid #f0f0f0;background:#fffef5}.tips-box .tips-inbox{width:80%;height:100%;margin:0 auto;display:flex;flex-direction:row;align-items:center}.tips-box .tips-inbox .icon{width:60rpx;height:60rpx;margin-right:30rpx}.tips-box .tips-inbox .tips-text{font-size:17px;font-weight:500;color:#a6a6a6}.gray-border-box{width:90%;height:100%;margin:0 auto;border:1px solid #bbb;border-radius:8px;padding:10rpx;background:#fff}.blue-column-border-box{width:90%;height:90rpx;margin:0 auto;display:flex;flex-direction:row;align-items:center;margin-top:10rpx}.blue-column-border-box .blue-border{width:5%;height:50rpx;border-left:6px solid #7ec0ee}.blue-column-border-box .blue-text{width:80%;font-size:18px;font-weight:700;color:#7ec0ee}.treat-record-box{width:90%;height:auto;margin:0 auto;background-color:#fff;border-radius:7px;overflow:hidden;margin-top:30rpx}.treat-record-box .info-box{width:90%;height:80rpx;margin:0 auto;line-height:80rpx;display:flex;flex-direction:row;justify-content:space-between}.treat-record-box .info-box .left{width:20%;height:100%;display:flex;justify-content:flex-start}.treat-record-box .info-box .right{width:80%;height:100%;display:flex;justify-content:flex-end}.treat-record-box .info-box .black-text{font-size:16px;font-weight:500;color:#000}.treat-record-box .info-box .gray-text{font-size:16px;font-weight:500;color:#a6a6a6}.treat-record-box .info-box:nth-last-child(2){border-bottom:1px solid #c8c7cc}.treat-record-box .more{width:90%;height:100rpx;margin:0 auto;line-height:100rpx;text-align:right;font-size:17px;font-weight:500;color:#7ec0ee}.treat-record-box:last-child{margin-bottom:30rpx}.loading{text-align:center;line-height:80px}